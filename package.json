    "astro": "^5.13.7",
    "astro-icon": "^1.1.5",
    "dotenv": "^17.2.2",
    "svelte": "^5.38.8",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@iconify-json/fa7-brands": "^1.2.0",
    "@iconify-json/heroicons": "^1.2.2",
    "daisyui": "^5.1.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14"
  }
